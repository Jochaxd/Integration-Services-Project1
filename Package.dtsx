                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select prodD.id as productoid, clid.id as ClienteID,  
empD.id as EmpleadoID, tiD.id TiempoID,  
ProvD.id ProveedorID, 
detoN.UnitPrice*detoN.Quantity as PrecioTotal,  
detoN.Quantity as Cantidad 
from northwind.dbo.products prodN  
inner join northwind.dbo.categories catN on (prodN.categoryid=catN.categoryid) 
inner join Northwind.dbo.suppliers provN on (prodN.supplierid=provN.supplierid) 
inner join Northwind.[dbo].[Order Details] detoN on (prodN.productid=detoN.productid) 
inner join Northwind.dbo.orders ordN on (ordN.orderid=deton.orderid) 
inner join Northwind.dbo.customers cliN on (cliN.customerid=ordN.customerid) 
inner join Northwind.dbo.employees empN on (empN.employeeid=ordN.employeeid) 
right join Northwind_DW.dbo.producto prodD on (prodD.nombre COLLATE Modern_Spanish_CI_AS =prodN.productname) 
right join Northwind_DW.dbo.cliente cliD on (cliD.nombre COLLATE Modern_Spanish_CI_AS =cliN.contactName) 
right join Northwind_DW.dbo.empleado empD on (empD.apellido COLLATE Modern_Spanish_CI_AS =empN.LastName) 
right join Northwind_DW.dbo.tiempo tiD on (tiD.fecha = convert(date,ordN.orderdate)) 
right join Northwind_DW.dbo.proveedor provD on (provD.nombrecompani COLLATE Modern_Spanish_CI_AS =provN.CompanyName)</property>
